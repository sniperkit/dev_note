{
  "id": "/spark/master",
  "cpus": 0.5,
  "mem": 1024,
  "disk": 1024,
  "networks": [ { "mode": "container/bridge" } ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "alpine/spark-2.3.0"
    },
    "portMappings": [
      { "containerPort": 8080, "hostPort": 0 }
    ],
    "command": "./sbin/start-master.sh && /bin/bash"
  }
}